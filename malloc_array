main
		sub		sp, sp, #12
		mov		r0, #400
		bl		malloc
		mov		r3, r0
		str		r3, [sp, #4]
		ldr		r0, [sp, #4]
		bl		free
		movs		r3, #0
		str		r3, [sp, #4]
		movs		r3, #0
		mov		r0, r3
		add		sp, sp, #12
		ldr		pc, [sp], #4
malloc
		adr		r4, memstore
		str		r0, [r4], r0
		add		r4, r4, #4
		mov		pc, lr
free
		sub		r5, r4, #4
		ldr		r4, [r5], -r0
		mov		pc, lr
memstore	FILL		1000
storesize	DCD		1000
